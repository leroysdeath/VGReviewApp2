{
  "name": "vgreviewapp2-production",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:full": "node scripts/start-dev.cjs",
    "netlify": "node scripts/cleanup-ports.cjs && netlify dev && start http://localhost:5173",
    "netlify:open": "node scripts/cleanup-ports.cjs && netlify dev && timeout 3 && start http://localhost:5173",
    "start": "node scripts/start-dev.cjs",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "build:analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "cleanup": "node scripts/cleanup-node-processes.cjs",
    "cleanup:ports": "node scripts/cleanup-node-processes.cjs",
    "kill-dev": "node scripts/cleanup-node-processes.cjs",
    "sync-igdb": "node scripts/sync-igdb.js",
    "sync-igdb:dry": "node scripts/sync-igdb.js --dry-run",
    "sync-igdb:help": "node scripts/sync-igdb.js --help",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:search": "jest src/test/search.test.ts",
    "test:fast": "jest --config jest.config.fast.js",
    "test:ci": "jest --config jest.config.ci.js",
    "test:dev": "jest --config jest.config.fast.js --watch",
    "test:parallel": "jest --maxWorkers=75%",
    "test:unit": "jest --testPathPatterns=\"(simple|basic|unit|fast)\" --maxWorkers=100%",
    "test:integration": "jest --testPathPatterns=\"(integration|search|game|franchise)\" --maxWorkers=50%",
    "test:advanced": "node -r ts-node/register src/test/advanced-runner.ts",
    "test:incremental": "node -r ts-node/register src/test/advanced-runner.ts incremental",
    "test:smart": "node -r ts-node/register src/test/advanced-runner.ts dev"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.1.1",
    "@supabase/supabase-js": "^2.50.3",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.1",
    "@use-gesture/react": "^10.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.3",
    "focus-trap-react": "^10.2.3",
    "immer": "^10.1.1",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.10.1",
    "react-slider": "^2.0.6",
    "react-swipeable": "^7.0.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "rollup-plugin-visualizer": "^6.0.3",
    "swr": "^2.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vite-imagetools": "^8.0.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^9.9.0",
    "@netlify/functions": "^2.8.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^26.1.0",
    "msw": "^2.11.1",
    "node-fetch": "^3.3.2",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "typescript-eslint": "^8.3.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-imagetools": "^8.0.0",
    "whatwg-fetch": "^3.6.20"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.19.11",
    "@rollup/rollup-linux-x64-gnu": "^4.9.5"
  }
}
